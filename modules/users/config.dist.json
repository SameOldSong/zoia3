{
    "collectionUsers": "users",
    "database": {
        "collections": {
            "users": {
                "indexesAsc": ["username", "email"],
                "indexesDesc": ["username", "email"]
            },
            "captcha": {
                "indexesAsc": [],
                "indexesDesc": [],
                "expires": 3600
            }
        }
    },
    "routes": {
        "admin": "/admin/users",
        "login": "/users/login",
        "logout": "/users/logout"
    },
    "defaults": {
        "username": "admin",
        "password": "password"
    },
    "setup": true
}
